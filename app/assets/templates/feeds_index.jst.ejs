<h2>All Feeds</h2>

<ul>
  <% feeds.each(function(feed) { %>
    <li>
      <a href='/#feeds/<%= feed.id %>'><%= feed.escape('title') %></a>
      <button class="delete" data-id='<%=feed.id%>'>Delete</button>
    </li>
  <% }); %>
</ul>

<h3>Add Feed</h3>
<form>
  <!-- <label>Title:
    <input type='text' name='feed[title]'>
  </label><br> -->

  <label>URL:
    <input type='text' name='feed[url]'>
  </label><br>

  <button>Add Feed</button>
</form>